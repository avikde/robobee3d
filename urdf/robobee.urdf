<robot name="robobee">
	
	<link name="body">
		<visual>
			<origin xyz="0 0 0.0" rpy="0 0 0"/>
			<geometry>
				<box size="0.05 0.05 0.2"/>
			</geometry>
	    <material name="Blue1">
				<color rgba="0 0 1 1.0"/>
			</material>
	   </visual>
	</link>

	<link name="lwing_spar">
		<visual>
			<origin xyz="0 -0.08 0.0" rpy="0 0 0"/>
			<geometry>
				<box size="0.01 0.16 0.01"/>
			</geometry>
	    <material name="Black1">
				<color rgba="0 0 0 1.0"/>
			</material>
	   </visual>
	</link>

	<link name="rwing_spar">
		<visual>
			<origin xyz="0 0.08 0.0" rpy="0 0 0"/>
			<geometry>
				<box size="0.01 0.16 0.01"/>
			</geometry>
	    <material name="Black1"/>
	   </visual>
	</link>

	<link name="lwing_membrane">
		<visual>
			<origin xyz="0 -0.08 -0.03" rpy="0 0 0"/>
			<geometry>
				<box size="0.01 0.12 0.06"/>
			</geometry>
	    <material name="Gray1">
				<color rgba="0 0 0 0.3"/>
			</material>
	   </visual>
	</link>

	<link name="rwing_membrane">
		<visual>
			<origin xyz="0 0.08 -0.03" rpy="0 0 0"/>
			<geometry>
				<box size="0.01 0.12 0.06"/>
			</geometry>
	    <material name="Gray1"/>
	   </visual>
	</link>
	
	<!-- * * * Joint Definitions * * * -->
	
	<joint name="lwing_stroke" type="continuous">
		<parent link="body"/>
		<child link="lwing_spar"/>
		<axis xyz="0 0 1"/>
		<origin xyz="0.0 0 0.1" rpy="0 0 0"/>
	</joint>

	<joint name="rwing_stroke" type="continuous">
		<parent link="body"/>
		<child link="rwing_spar"/>
		<axis xyz="0 0 -1"/>
		<origin xyz="0.0 0 0.1" rpy="0 0 0"/>
	</joint>
	
	<joint name="lwing_hinge" type="continuous">
		<parent link="lwing_spar"/>
		<child link="lwing_membrane"/>
		<axis xyz="0 1 0"/>
		<origin xyz="0.0 0 0.0" rpy="0 0 0"/>
	</joint>
	
	<joint name="rwing_hinge" type="continuous">
		<parent link="rwing_spar"/>
		<child link="rwing_membrane"/>
		<axis xyz="0 -1 0"/>
		<origin xyz="0.0 0 0.0" rpy="0 0 0"/>
	</joint>

</robot>