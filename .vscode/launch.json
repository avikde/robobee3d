	"version": "0.2.0",
	"configurations": [
		{
			"cwd": "${workspaceRoot}/h7bee",
			"executable": "build/h7bee.elf",
			"name": "debug h7bee",
			"request": "launch",
			"type": "cortex-debug",
			"servertype": "openocd",
			"device": "STM32H743ZI",
			"interface": "swd",
			"runToMain": true,
			"preLaunchTask": "build h7bee",
			"configFiles": [
				"board/st_nucleo_h743zi.cfg"
			],
		},
		{
			"cwd": "${workspaceRoot}/planar/mcuqp/build",
			"executable": "main.elf",
			"name": "mcuqp debug",
			"request": "launch",
			"type": "cortex-debug",
			"servertype": "openocd",
			"device": "STM32F446VE",
			"interface": "swd",
			"configFiles": [
				"board/mbm.cfg"
			],
			"svdFile": "${workspaceRoot}/src/svd/STM32F446.svd",
			"preLaunchTask": "build mcuqp",
		},
	]
}