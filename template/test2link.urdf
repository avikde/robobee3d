<?xml version="1.0" ?>
<robot name="test2link">
  <!-- Body -->
  <link name="body">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 1"/>
      </geometry>
    </visual>
  </link>
  <link name="link1">
    <visual>
      <origin rpy="0 0 0" xyz="0 0.5 0"/>
      <geometry>
        <box size="0.1 1 0.1"/>
      </geometry>
    </visual>
  </link>
  <link name="link2">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.5"/>
      </geometry>
    </visual>
  </link>
  
  <joint name="0" type="continuous">
    <parent link="body"/>
    <child link="link1"/>
    <axis xyz="0 0 1"/>
    <origin rpy="0 0 0" xyz="0.0 0.1 0.5"/>
  </joint>
  <joint name="1" type="continuous">
    <parent link="link1"/>
    <child link="link2"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 0" xyz="0 1 -0.3"/>
  </joint>
</robot>
